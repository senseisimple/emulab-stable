			import mx.controls.Label;
			import mx.events.CloseEvent;
			import mx.managers.PopUpManager;
			
			public var main:pgmap;
			
			public function loadLink(l:Link):void {
				linkInfo.load(l);
			}
		]]>
	</mx:Script>
	<LinkAdvancedInformation  height="100%" width="100%" verticalGap="0" paddingTop="0" paddingRight="0" paddingLeft="0" paddingBottom="0" horizontalGap="0" id="linkInfo"/>
